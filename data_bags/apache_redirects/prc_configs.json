{
  "id": "prc_configs",
  "rewrites" : [ "^/robots.txt$ - [L]",
    "^/fsgbb241.txt$ - [L]",
    "^/jmherror/(.*) /jmherror/$1 [PT,L]" ], 
  "rewrites_ssl" : [ "^/robots.txt$ - [L]",
    "^/fsgbb241.txt$ - [L]",
    "^/jmherror/(.*) /jmherror/$1 [PT,L]",
    "^/medstaff/payments/concord(.*) /content/prc/en/home/public/payments/concord.html [PT,L]",
    "^/medstaff/payments/walnut-creek(.*) /content/prc/en/home/public/payments/walnut-creek.html [PT,L]",
    "^/*$ /content/prc/en/home.html [PT,L]" ],
  "ssl_redirects": [ "%{REQUEST_URI} ^/(.*)" ],
  "cq_ignore_content_https": [ "%{REQUEST_URI} !/dispatcher/invalidate.cache$",
      "%{REQUEST_URI} !/server-status.*$",
      "%{REQUEST_URI} !/apps",
      "%{REQUEST_URI} !/bin",
      "%{REQUEST_URI} !/etc",
      "%{REQUEST_URI} !/home",
      "%{REQUEST_URI} !/libs",
      "%{REQUEST_URI} !/tmp",
      "%{REQUEST_URI} !/var",
      "%{REQUEST_URI} !^/content/dam/.*$",
      "%{REQUEST_URI} !^/system.*" ]
}
